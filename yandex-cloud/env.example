# Yandex Cloud Configuration
# Скопируйте этот файл в .env и заполните значения

# OpenAI API
OPENAI_API_KEY=your-openai-api-key-here

# Yandex Managed Service for Redis
REDIS_HOST=your-redis-host.c.yandexcloud.net
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password-here

# Google Apps Script (опционально, можно переопределить через dataset.gas)
GAS_ENDPOINT=https://script.google.com/macros/s/YOUR-GAS-URL/exec

# Yandex Cloud Functions (для деплоя)
YC_CLOUD_ID=your-cloud-id
YC_FOLDER_ID=your-folder-id
YC_SERVICE_ACCOUNT_KEY=path-to-service-account-key.json

# Object Storage
OBJECT_STORAGE_BUCKET=your-bucket-name

# Server Port (для локальной разработки)
PORT=3001

